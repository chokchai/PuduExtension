            "matches": ["http://*.lolthai.com/*"],
            "css": [
                "libs/jquery.ui.totop.css",
                "libs/font-awesome.min.css",
                "pudu.min.css"
            ],
            "js": [
                "libs/jquery-2.0.3.min.js",
                "libs/jquery.ui.totop.min.js",
                "utils.min.js",
                "pudu.min.js"
            ]
        }
    ],

    "icons" : {
        "16": "imgs/pudu-icon-16.png",
        "48": "imgs/pudu-icon-48.png",
        "128": "imgs/pudu-icon-128.png"
    },

